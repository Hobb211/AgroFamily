<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#FDCB9E</Color>
    <Color x:Key="primaryBackColor2">#E94256</Color>
    <Color x:Key="primaryBackColor3">#263135</Color>
    
    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#E94256</Color>
    <Color x:Key="winBorderColor2">#fd7e74</Color>
    <Color x:Key="winBorderColor3">#FDCB9E</Color>
    
    <!--Color Palette-->
    <SolidColorBrush x:Key="Color1" Color="#EB1000"/>
    <SolidColorBrush x:Key="Color2" Color="#7c7673"/>
    <SolidColorBrush x:Key="Color3" Color="#0088de"/>
    <SolidColorBrush x:Key="Color4" Color="#fd7e74"/>

    <!--Font Colors-->
    <SolidColorBrush x:Key="textColor1" Color="White"/>
    <SolidColorBrush x:Key="textTitleColor1" Color="White"/>
           
    <!--Button Colors-->
    <SolidColorBrush x:Key="buttonColor1" Color="#505B60"/>
    <SolidColorBrush x:Key="buttonColor2" Color="#263135"/>
    

</ResourceDictionary>