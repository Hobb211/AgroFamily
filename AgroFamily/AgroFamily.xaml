<Window x:Class="AgroFamily.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgroFamily"
        mc:Ignorable="d"
        Title="AgroFamily" Height="450" Width="800"
        Background="Gray">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Language="es-cl" Text="AgroFamily"/></TextBlock>
        <Button Content="Registrarse" Click="RegisterButton" Margin="0,50,0,0" Height="38" VerticalAlignment="Center" HorizontalAlignment="Center" Width="76"/>
        <Button Content="Ingresar" Click="LoginButton" Margin="0,-50,0,0" HorizontalAlignment="Center" Width="76" Height="38" VerticalAlignment="Center"/>

    </Grid>
</Window>
